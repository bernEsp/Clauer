<% if(collection.models.length > 0){ %>
  <h3><a href='#new'>Create New</a></h3>
  <ul>
  <%collection.each(function(key){%>
    <li>
      <a href='#keyrepos/" + key.id + "'><%= key.escape('name') %></a>
    </li>
  <%});%> 
<%}else{%>
   <h3>No documents! 
     <a href='#new'>Create one</a>
   </h3>
<%}%>